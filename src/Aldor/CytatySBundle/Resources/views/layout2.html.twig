<!DOCTYPE html>
<html lang="en">

<head>

<meta charset="utf-8">
<title>{% block title %}Twitter Bootstrap for Symfony2{% endblock %}</title>
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<meta name="description" content="Twitter Bootstrap for Symfony2 brings the famous Twitter Bootstrap to Symfony2. In a snap.">
<meta name="author" content="Florian Eckerstorfer">

<!-- Use precompiled CSS files -->
<!-- {{ "{% include 'BraincraftedBootstrapBundle::_stylesheets.html.twig' %}" }} -->
<link rel="stylesheet" href="{{ asset('css/bootstrap.css') }}">

<!-- Use Assetic -->
<!-- {{ "<link rel=\"stylesheet\" href=\"{{ asset('bundles/braincraftedbootstrap/css/bootstrap.compiled.css') }}\">" }} -->

<link rel="stylesheet" href="{{ asset('bundles/aldorcytatys/css/docs.css') }}">
<link rel="stylesheet" href="{{ asset('bundles/aldorcytatys/css/bc-docs.css') }}">
<link rel="stylesheet" href="{{ asset('bundles/aldorcytatys/css/google-code-prettify.css') }}">

{% include 'BcBootstrapBundle::_html5shiv.html.twig' %}

</head>

<body data-spy="scroll" data-target=".bs-docs-sidebar" data-twttr-rendered="true">

    <!-- Navbar
    ================================================== -->
    <div class="navbar navbar-inverse navbar-fixed-top">
      <div class="navbar-inner">
        <div class="container">
          <button type="button" class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>

          <a class="brand" href="/">Strims Cytaty</a>
          <div class="nav-collapse collapse">
            {{ knp_menu_render('AldorCytatySBundle:Builder:navbar', {'nav_type': 'navbar'}) }}
          </div>
        </div>
      </div>
    </div>

{% block body %}{% endblock %}

    <!-- Footer
    ================================================== -->
    <footer class="footer">
      <div class="container">
        <p class="pull-right"><a href="#">Back to top</a></p>
      </div>
    </footer>

<!-- Use precompiled JavaScript files (includes jQuery) -->
<!-- {{ "{% include 'BraincraftedBootstrapBundle::_javascripts.html.twig' %}" }} -->

<!-- Use Assetic -->
<script src="//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
<script>window.jQuery || document.write('<script src="{{ asset('js/jquery.js') }}"><\/script>')</script>
<script src="{{ asset('js/bootstrap.js') }}"></script>
<!-- {{ "<script src=\"{{ asset('bundles/braincraftedbootstrap/js/bootstrap.compiled.js') }}\"></script>" }}-->

<script src="{{ asset('bundles/aldorcytatys/js/application.js') }}"></script>
<script src="{{ asset('bundles/aldorcytatys/js/google-code-prettify.js') }}"></script>
<script type="text/javascript" src="http://platform.twitter.com/widgets.js"></script>

</body>

</html>
